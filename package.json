    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "coveralls": "2.13.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.4",
    "nyc": "10.2.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "consign": "^0.1.6",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
